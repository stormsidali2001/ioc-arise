{
  "name": "ioc-maker",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "changeset": "changeset",
    "changeset:done": "npm run changeset && npm run changeset version && npm run changeset publish "
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "prettier": "^3.5.3",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "nanoid": "^4.0.2"
    }
  }
}